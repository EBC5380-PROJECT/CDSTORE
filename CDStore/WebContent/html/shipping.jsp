<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<link rel="icon" href="../../favicon.ico">

	<title>shipping address</title>

	<!-- Bootstrap core CSS -->
	<link href="../css/bootstrap.min.css" rel="stylesheet">
	<!-- Custom styles for this template -->
	<link href="../css/dashboard.css" rel="stylesheet">
	<script src="../lib/ie-emulation-modes-warning.js"></script>
	<script src="../lib/jquery-2.1.4.min.js"></script>
	<script src="../js/host.js"></script>
	<script>
		//change the form's action dynamically on page load such that there is
		var changeAction = function () {
			$("#form").attr("action", name + Server.testService.url)
		};

		var error = "";

		$(function () {
			//change the form action dynamically
			changeAction();

			//access error message and display them
			/*<% 
				String error = session.getAttribute("error");
			%>
			//error = "<%=error %>"
			*/

			displayError();
		});
	</script>

</head>

<body>

	<div class="container" style="padding:20px 400px 10px 400px ; ">
		<form class="form-register" id="form" action="" method="post">
			<h3 class="form-register-heading">Check out: shipping address</h3>
			<!-- Error messages display here -->
			<font color="red" id="msg_error">
		  	
		  </font>

			<div class="form-group">
				<label for="Full name ">Full name:</label>
				<input type="Full name" class="form-control" id="Full name" placeholder="Your full name" name="name" required>
			</div>

			<div class="form-group">
				<label for="Address Line 1">Address:</label>
				<input type="Address Line 1" class="form-control" id="Address Line 1" placeholder="Your address" name="address" required>
			</div>

			<div class="form-group">

				<div class="form-group">
					<label for="City">City: </label>
					<input type="City" class="form-control" id="City" placeholder="City" name="city" required>
				</div>

				<div class="form-group">
					<label for="Province">Province: </label>
					<select class="selectpicker" style="width:375px; height:36px;" name="province" required>
						<option></option>
						<option>Alberta</option>
						<option>British Columbia</option>
						<option>New Brunswick</option>
						<option>Newfoundland</option>
						<option>Northwest Territories</option>
						<option>Nova Scotia</option>
						<option>Nunavut</option>
						<option>Ontario</option>
						<option>Prince Edward Island</option>
						<option>Quebec</option>
						<option>Saskatchewan</option>
						<option>Yukon</option>
					</select>
				</div>

				<div class="form-group">
					<label for="Postal Code">Postal Code: </label>
					<input type="Postal Code" class="form-control" id="Postal Code" placeholder="Your postal code" name="postalcode" required>
				</div>

				<div class="form-group">
					<label for="Telephone number">Telephone number: </label>
					<input type="Telephone number" class="form-control" id="Telephone number" placeholder="Your phone number" name="phone" required>
				</div>
				<br>
				<button class="btn  btn-warning btn-block" type="submit">Ship to this address</button>
		</form>
		</div>





		<div id="footer" style="margin-left:500px;">
			kill -9 &copyCopyright 2015 - Do not reproduce or plagarize.
		</div>
		<script src="../lib/bootstrap.min.js"></script>
		<!-- Just to make our placeholder images work. Don't actually copy the next line! -->
		<script src="../lib/vendor/holder.min.js"></script>
		<!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
		<script src="../lib/ie10-viewport-bug-workaround.js"></script>
</body>

</html>