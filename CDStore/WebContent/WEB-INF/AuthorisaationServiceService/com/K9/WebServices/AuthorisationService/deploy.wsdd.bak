<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AuthorisaationServiceService WSDL service -->

  <service name="AuthorisaationService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://AuthorisationService.WebServices.K9.com"/>
      <parameter name="wsdlServiceElement" value="AuthorisaationServiceService"/>
      <parameter name="schemaQualified" value="http://AuthorisationService.WebServices.K9.com"/>
      <parameter name="wsdlServicePort" value="AuthorisaationService"/>
      <parameter name="className" value="com.K9.WebServices.AuthorisationService.AuthorisaationServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="AuthorisaationService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="authorisePurchase" qname="operNS:authorisePurchase" xmlns:operNS="http://AuthorisationService.WebServices.K9.com" returnQName="retNS:authorisePurchaseReturn" xmlns:retNS="http://AuthorisationService.WebServices.K9.com" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:paymentInfo" xmlns:pns="http://AuthorisationService.WebServices.K9.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:order" xmlns:pns="http://AuthorisationService.WebServices.K9.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="authorisePurchase"/>

  </service>
</deployment>
